[flake8]
select = ANN,B,B9,BLK,C,D,DAR,E,F,I,S,W
ignore = E203,E501,W503,ANN101,ANN102,S322,ANN206,S201,D105,D107,ANN401
per-file-ignores =
    src/replit/__init__.py:F401
    src/replit/web/__init__.py:F401
    tests/test_identity.py:S105,S106
exclude =
    src/replit/goval/api

max-line-length = 88
application-import-names = vidgen,tests
import-order-style = google
docstring-convention = google
